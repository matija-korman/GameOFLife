<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="application.Cell" table="CELLS_LIST" >
        <id name="cellId" column="CELLS_ID" type="long" >
            <generator class="native" />
        </id>
        <property name="alive" type="boolean" column="LIFE_STATUS"/>
         
    </class>
</hibernate-mapping>